SIGNAL PROCESS
F:
       "LINE" ROW0 " = LINE + <" COL0 "  'FFFFFFFFFFFF'>"
       "LINE" ROW1 " = LINE + <" COL1 "  'FFFFFFFFFFFF'>"
       "LINE" ROW2 " = LINE + <" COL2 "  'FF'>"
       "LINE" ROW3 " = LINE + <" COL3 "  'FF'>"
       "LINE" ROW4 " = LINE + <" COL4 "  'FF'>"
       "LINE" ROW5 " = LINE + <" COL5 "  'FFFFFFFF'>"
       "LINE" ROW6  "= LINE + <" COL6 "  'FFFFFFFF'>"
       "LINE" ROW7  "= LINE + <" COL7 "  'FF'>"
       "LINE" ROW8  "= LINE + <" COL8 "  'FF'>"
       "LINE" ROW9  "= LINE + <" COL9 "  'FF'>"
       "LINE" ROW10  "= LINE + <" COL10" 'FF'>"
       "LINE" ROW11  "= LINE + <" COL11" 'FF'>"
SIGNAL PROCESS
