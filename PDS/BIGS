       "LINE" ROW3 " = LINE + <" COL3 "  'DD        DD'>"
       "LINE" ROW4 " = LINE + <" COL4 "  'DD        DD'>"
       "LINE" ROW5 " = LINE + <" COL5 "  'DD        DD'>"
       "LINE" ROW6  "= LINE + <" COL6 "  'DD        DD'>"
       "LINE" ROW7  "= LINE + <" COL7 "  'DD        DD'>"
       "LINE" ROW8  "= LINE + <" COL8 "  'DD        DD'>"
       "LINE" ROW9  "= LINE + <" COL9 "  'DD       DD'>"
