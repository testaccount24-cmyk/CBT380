       "LINE" ROW3 " = LINE + <" COL3 "  'GG'>"
       "LINE" ROW4 " = LINE + <" COL4 "  'GG'>"
       "LINE" ROW5 " = LINE + <" COL5 "  'GG'>"
       "LINE" ROW6  "= LINE + <" COL6 "  'GG     GGGGG'>"
       "LINE" ROW7  "= LINE + <" COL7 "  'GG     GGGGG'>"
       "LINE" ROW8  "= LINE + <" COL8 "  'GG        GG'>"
       "LINE" ROW9  "= LINE + <" COL9 "  'GG        GG'>"
       "LINE" ROW10  "= LINE + <" COL10" 'GGGGGGGGGGGG'>"
