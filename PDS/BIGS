       "LINE" ROW7  "= LINE + <" COL7 "  '            '>"
       "LINE" ROW8  "= LINE + <" COL8 "  '            '>"
       "LINE" ROW9  "= LINE + <" COL9 "  '            '>"
       "LINE" ROW10  "= LINE + <" COL10" '    ...     '>"
       "LINE" ROW11  "= LINE + <" COL11" '    ...     '>"
SIGNAL PROCESS
X79:
       "LINE" ROW0 " = LINE + <" COL0 "  '   ``       '>"
       "LINE" ROW1 " = LINE + <" COL1 "  '     ``     '>"
       "LINE" ROW2 " = LINE + <" COL2 "  '       ``   '>"
       "LINE" ROW3 " = LINE + <" COL3 "  '            '>"
       "LINE" ROW4 " = LINE + <" COL4 "  '            '>"
       "LINE" ROW5 " = LINE + <" COL5 "  '            '>"
       "LINE" ROW6  "= LINE + <" COL6 "  '            '>"
       "LINE" ROW7  "= LINE + <" COL7 "  '            '>"
       "LINE" ROW8  "= LINE + <" COL8 "  '            '>"
       "LINE" ROW9  "= LINE + <" COL9 "  '            '>"
       "LINE" ROW10  "= LINE + <" COL10" '            '>"
       "LINE" ROW11  "= LINE + <" COL11" '            '>"
 SIGNAL PROCESS

/*********************************************************************/
/*   P R O C E S S :    SETTING UP FOR THE NEXT LETTER IF ANY        */
/*********************************************************************/
PROCESS:
    IF  MAXCC  > 0 THEN DO
       ZEDSMSG = "INCOMPLETE BOX"
       ZEDLMSG = "NOT ENOUGH LINES/COLUMNS",
          "TO DRAW COMPLETE BOX MAX="MAXCC
       ADDRESS "ISPEXEC" "SETMSG MSG(ISRZ001)"
              "(ERRSS) = LINENUM .ERRSS"
       IF  ERRSS  <> 0 THEN        "LABEL .SSS = .ERRSS 0"
    END

    ROW =      ROW +  VERT

/*********************************************************************/
/*  ADJUSTING THE END PLACEMENT OF THE CURSOR, NOT A VERY PRECISE    */
/*  MECHANISM BUT IT SHOULD WORK WELL TO KEEP LINES REASONABLY       */
/*  SPREAD APART WITH THE SAME FEATURES.                             */
/*********************************************************************/

  IF     LETTERX     ^= ""     THEN SIGNAL MORE

  IF VERT < 0 THEN AVERT = -   VERT
  IF SLANT > 0 THEN DO
     IF  VERT   > 0 THEN ROWN = ROWN +    VERT
     IF  VERT   < 0 THEN ROWN = ROWN -    VERT
  END
  IF SLANT < 0 THEN DO
     IF  VERT   > 0 THEN ROWN = ROWN +    VERT
     IF  VERT   < 0 THEN ROWN = ROWN -    VERT
  END

   IF 1 = NOTES THEN DO
       "LINE_BEFORE .ZFIRST = NOTELINE """||,
           "------ SAMPLE USAGE OF -- BIGS -- CLIST"""
       "LINE_BEFORE .ZFIRST = NOTELINE """||,
           "CURSOR MUST BE PLACED ON A LINE, UNLESS MEMBER IS EMPTY"""
       "LINE_BEFORE .ZFIRST = NOTELINE ""BIGS <SPACE BETWEEN",
           "CHAR|1|4> <SLANT|0> <VERT|0> LETTERS"""
        "LINE_BEFORE .ZFIRST = NOTELINE "" E.G. BIGS 1 0 0 HELLO"""
        "LINE_BEFORE .ZFIRST = NOTELINE """||,
           "    E.G.  BIGS 1 1 1 DEMO"""
        "LINE_BEFORE .ZFIRST = NOTELINE """||,
           "    E.G.  BIGS 1 -1 1 A B C"""
  END
         "LABEL" ROWN "= .NCUR 0"
         "LOC" ROWN
         "UP 18"
         ROWN=ROWN+0;COLN=COLN+0
         "CURSOR =" ROWN COLN
  /********************** END OF 1991/04/29 ADDITIONS */
  EXIT
