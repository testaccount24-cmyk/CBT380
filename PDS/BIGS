       "LINE" ROW0 " = LINE + <" COL0 "  'EEEEEEEEEEEE'>"
       "LINE" ROW1 " = LINE + <" COL1 "  'EEEEEEEEEEEE'>"
       "LINE" ROW2 " = LINE + <" COL2 "  'EE'>"
       "LINE" ROW3 " = LINE + <" COL3 "  'EE'>"
       "LINE" ROW4 " = LINE + <" COL4 "  'EE'>"
       "LINE" ROW5 " = LINE + <" COL5 "  'EEEEEEEE'>"
       "LINE" ROW6  "= LINE + <" COL6 "  'EEEEEEEE'>"
       "LINE" ROW7  "= LINE + <" COL7 "  'EE'>"
       "LINE" ROW8  "= LINE + <" COL8 "  'EE'>"
       "LINE" ROW9  "= LINE + <" COL9 "  'EE'>"
       "LINE" ROW10  "= LINE + <" COL10" 'EEEEEEEEEEEE'>"
