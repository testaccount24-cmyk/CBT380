       "LINE" ROW0 " = LINE + <" COL0 "  ' CCCCCCCCCC'>"
       "LINE" ROW1 " = LINE + <" COL1 "  'CCCCCCCCCCCC'>"
       "LINE" ROW2 " = LINE + <" COL2 "  'CC        CC'>"
       "LINE" ROW3 " = LINE + <" COL3 "  'CC'>"
       "LINE" ROW4 " = LINE + <" COL4 "  'CC'>"
       "LINE" ROW5 " = LINE + <" COL5 "  'CC'>"
       "LINE" ROW6  "= LINE + <" COL6 "  'CC'>"
       "LINE" ROW7  "= LINE + <" COL7 "  'CC'>"
       "LINE" ROW8  "= LINE + <" COL8 "  'CC'>"
       "LINE" ROW9  "= LINE + <" COL9 "  'CC        CC'>"
       "LINE" ROW10  "= LINE + <" COL10" 'CCCCCCCCCCCC'>"
       "LINE" ROW11  "= LINE + <" COL11" ' CCCCCCCCCC'>"
SIGNAL PROCESS
