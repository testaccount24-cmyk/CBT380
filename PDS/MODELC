  PROC 1 MEMBER EDIT
       SET BROWSE = BROWSE
       IF &SYSCAPS(&STR(&EDIT)) = EDIT THEN SET BROWSE = EDIT
       IF &STR(&MEMBER.) = &STR(*) THEN DO
          SET CHK =  &STR(&SYSDSN('TSOMODEL.PUBLIC.CNTL')
          IF &STR(&CHK.) = OK THEN -
ISPEXEC           &BROWSE DATASET('TSOMODEL.PUBLIC.CNTL')
          ELSE -
                  WRITE:  DATASET 'TSOMODEL.PUBLIC.CNTL' &STR(&CHK.)
          EXIT CODE(0)
       END
       SET CHK =  &STR(&SYSDSN('TSOMODEL.PUBLIC.CNTL(&MEMBER.)')
       IF &STR(&CHK.) = OK THEN -
ISPEXEC     &BROWSE DATASET('TSOMODEL.PUBLIC.CNTL(&MEMBER.)')
       ELSE -
       DO
     WRITE : YOU CAN BROWSE ALL MEMBERS IN TURN USING
     WRITE :                TSO MODELC *
     WRITE
     WRITE
     WRITE :  MEMBER &MEMBER NOT FOUND - GIVING YOU BACK THE DIRECTORY
     WRITE :  SAME AS IF YOU USED -      TSO MODELC *
ISPEXEC &BROWSE DATASET('TSOMODEL.PUBLIC.CNTL')
       END
 EXIT CODE(0)
