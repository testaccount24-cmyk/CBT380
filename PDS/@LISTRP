/*REXX -- MAKE  %LISTRP  LISTING INTO SCRIPTABLE MATERIAL*/
ADDRESS "ISREDIT";"MACRO (PARM)"
PARM = TRANSLATE(PARM)
"LINE_AFTER .ZL = DATALINE "".* ===> @SCRIPT DOWN  ",
     "--Corporate SITE ONLY--FP(1)"""
"LINE_AFTER .ZL = DATALINE "".* CREATED BY @LISTRP CLIST"""
"LINE_AFTER .ZL = DATALINE ""                           """
"(VAR037) = LINENUM .ZLAST";FIRST = VAR037 + 4
"@STRAP  LISTRP " PARM " DISPLAY"
"LABEL" VAR037 "= .A      0"
"LABEL" ".ZLAST = .B      0"
"UNGML"
"LINE_BEFORE .a = DATALINE "".fo off fold"""
"LINE_BEFORE .A = DATALINE "".dm separate on"""
"LINE_BEFORE .A = DATALINE "".hr left right;.kp off"""
"LINE_BEFORE .A = DATALINE "".if 38PP = &&&&$PDEV.",
     ||";.th .kp on;.el .kp 30"""
"LINE_BEFORE .A = DATALINE "".hr left right"""
"LINE_BEFORE .A = DATALINE "".dm off"""
"LINE_BEFORE .A = DATALINE "".if 38PPx = &&&&$PDEV.x;.th .pm .3i .3i"""
"LINE_BEFORE .A = DATALINE "".th .bf gt20 ="""
"LINE_BEFORE .A = DATALINE "".llmax;.twocol"""
"LINE_BEFORE .A = DATALINE "".vr &&&&$CL.;.ir 2"""
"LINE_BEFORE .A = DATALINE "".sv off"""
"LINE_BEFORE .A = DATALINE "" Created using ==> @LISTRP " PARM """"
"LINE_BEFORE .A = DATALINE ""   by " SYSVAR(SYSUID) " on ",
    DATE('U') " at " TIME('N')""""
"x all"
"change 1 .a .b all '* * * * * * * *' '.separate;.* * * *'"
"c all .a .b nx p'^' ' '"
"c all .a .b 1 nx ' ' '.separate;.* -- * * * * * * * * * *'"
"x all"
"find .a .b all "" NO CATEGORIES"""
"find .a .b all "" NO SECURITY LEVEL"""
"find .a .b all "" NO SECURITY LEVEL"""
"find .a .b all "" NO ENTRIES IN CONDITIONAL ACCESS LIST"""
"nxnx before(2) after(1)"
"del nx all"
"find .a .b all "" NO installation data"""
"find .a .b all ""COMMAND INVOKING @STRAP CLIST MACRO"""
"find .a .b all ""==> TSO LISTRP"""
"nxnx after(1)"
"del nx all"
"Find 'ARE NOT RACF PROTECTED'"
IF RC = 0 then
  "LINE_BEFORE .ZCSR = DATALINE " ,
     """.hr left right;.kp off;.vr off;.sc;.sk 1 p"""
