  PROC 1 MEMBER
  /* TSO   PROD01  -- EDIT THE PRODUCTION MEMBER SPECIFIED  */
  /* MACRO @PROD01 -- COPY THE PRODUCTION MEMBER SPECIFIED  TO END OF MEMBER IN
  IF &STR(&MEMBER) = &STR(PRODLST) THEN DO
  /* FREE DS(SPROD.TEMP)
     ALLOCATE DDNAME(EDITPDS) REUSE DSNAME(SPROD.TEMP) -
         NEW UNIT(TESTDISK)  SPACE(28,7) DIR(1) -
         LIKE('O010.LIBR.PROD')
     COPY 'O010.LIBR.PROD(PROD1)' SPROD.TEMP(PROD1) NONUM
     ISPEXEC EDIT DATASET(SPROD.TEMP(PROD1))
     DEL SPROD.TEMP
     FREE DS(SPROD.TEMP)
     EXIT CODE(&LASTCC)
  END
  SET MULTIMEM = &SYSINDEX(&STR(*),&STR(&MEMBER.))
  IF &MULTIMEM = 0 THEN -
     SET MULTIMEM = &SYSINDEX(&STR(%),&STR(&MEMBER.))
  ISPEXEC CONTROL ERRORS RETURN
  IF &STR(&MEMBER) = &STR(*) THEN -
     ISPEXEC     EDIT DATASET('TSOPROD.PROD01.CNTL') MACRO(@DIRSTAT)
  ELSE -
     ISPEXEC     EDIT DATASET('TSOPROD.PROD01.CNTL(&MEMBER)') MACRO(@DIRSTAT)
  SET &ZEDSMSG = &ZERRSM.
  SET &ZEDLMSG = &ZERRMAG &ZERRLM.
  ISPEXEC     SETMSG MSG(ISRZ000)
  CONTROL NOMSG
  FREE DATASET('TSOPROD.PROD01.CNTL')
  CONTROL MSG
